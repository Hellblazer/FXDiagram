            class="org.eclipse.xtext.example.fowlerdsl.ui.StatemachineExecutableExtensionFactory:de.fxdiagram.xtext.glue.commands.ShowInDiagramHandler"
            commandId="de.fxdiagram.xtext.glue.showInDiagramCommand">
            <activeWhen>
               <reference
                     definitionId="org.eclipse.xtext.example.fowlerdsl.Statemachine.Editor.opened">
               </reference>
            </activeWhen>
      </handler>
   </extension>
   <extension
         point="de.fxdiagram.xtext.glue.fxDiagramConfig">
      <config
            class="org.eclipse.xtext.example.fowlerdsl.ui.StatemachineExecutableExtensionFactory:de.fxdiagram.xtext.fowlerdsl.StatemachineDiagramConfig">
      </config>
   </extension>
</fragment>
