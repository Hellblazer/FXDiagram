            class="org.eclipse.xtext.example.domainmodel.ui.DomainmodelExecutableExtensionFactory:de.fxdiagram.xtext.glue.commands.ShowInDiagramHandler"
            commandId="de.fxdiagram.xtext.glue.showInDiagramCommand">
            <activeWhen>
               <reference
                     definitionId="org.eclipse.xtext.example.domainmodel.Domainmodel.Editor.opened">
               </reference>
            </activeWhen>
      </handler>
   </extension>
   <extension
         point="de.fxdiagram.xtext.glue.fxDiagramConfig">
